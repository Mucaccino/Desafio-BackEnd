{
  "metadata": [
    {
      "src": [
        {
          "src": "./",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "namespaceLayout": "flattened",
      "enumSortOrder": "declaringOrder",
      "outputFormat": "mref",
      "dest": "api"
    },
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "src": "./"
        }
      ],
      "namespaceLayout": "nested",
      "enumSortOrder": "declaringOrder",
      "outputFormat": "apiPage",
      "dest": "apipage"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "restapi/**",
          "apipage/**"          
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "makefile",
          "restapi/clients.ts"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Desafio Back-End",
      "_appTitle": "Desafio Back-End",
      "_enableSearch": true,
      "pdf": false
    },
    "keepFileLink": false,
    "exportViewModel": true,
    "postProcessors": []
  }
}